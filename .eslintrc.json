{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "consistent-return": 2,
        "indent": [
            1,
            4
        ],
        "no-else-return": 1,
        "semi": [
            1,
            "always"
        ],
        "space-unary-ops": 2
    }
}